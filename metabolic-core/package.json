{
  "name": "metabolic-core",
  "version": "1.0.0",
  "description": "行健行为教练 - 代谢慢病行为健康决策系统内核",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "ts-node src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "start:api": "ts-node src/main.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/database/config/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/database/config/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/database/config/data-source.ts",
    "schema:sync": "npm run typeorm -- schema:sync -d src/database/config/data-source.ts"
  },
  "keywords": [
    "behavioral-health",
    "metabolic",
    "cgm",
    "intervention",
    "coach-training",
    "phenotype-mapping",
    "trajectory"
  ],
  "author": "XingJian Behavioral Coach",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
