{
  "credential_types": {
    "physician_license": {
      "target_role": "supervisor",
      "review_interval_days": 365,
      "auto_expire": true,
      "display_name": "执业医师资格"
    },
    "coach_certification": {
      "target_role": "coach",
      "review_interval_days": 730,
      "auto_expire": false,
      "display_name": "教练认证"
    },
    "phd_supervision": {
      "target_role": "supervisor",
      "review_interval_days": 365,
      "auto_expire": true,
      "display_name": "博导/硕导资质"
    }
  },
  "forced_agents": ["crisis", "supervisor_reviewer"],
  "default_enabled_agents_by_role": {
    "coach": ["crisis", "behavior_coach", "metabolic_expert", "nutrition_expert", "exercise_expert", "sleep_expert"],
    "supervisor": ["crisis", "supervisor_reviewer", "behavior_coach", "metabolic_expert", "cardiac_rehab", "nutrition_expert", "exercise_expert", "sleep_expert", "tcm_expert", "mental_expert"]
  },
  "evidence_tiers": {
    "T1": {"label": "Meta分析/系统评价", "confidence_multiplier": 1.0, "min_role_to_create": "admin"},
    "T2": {"label": "RCT/队列研究", "confidence_multiplier": 0.9, "min_role_to_create": "supervisor"},
    "T3": {"label": "专家共识/指南", "confidence_multiplier": 0.75, "min_role_to_create": "coach"},
    "T4": {"label": "自定义/未验证", "confidence_multiplier": 0.5, "min_role_to_create": "coach"}
  }
}
