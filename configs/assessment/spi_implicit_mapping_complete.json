{
  "version": "1.0.0",
  "name": "spi_implicit_data_mapping",
  "description": "SPI评估问卷50题与隐性数据源的完整映射规则",
  "compatible_with": {
    "behavior_rules": "1.0.0",
    "state_sync_manager": "1.0.0",
    "trigger_engine": "1.0.0"
  },
  
  "stage_mapping": {
    "description": "心理层次(L1-L5)与行为阶段(S0-S5)的双向映射",
    "psychological_to_behavioral": {
      "L1": {"primary": "S0", "secondary": null, "coefficient": 0.3, "display_name": "探索期"},
      "L2": {"primary": "S0", "secondary": "S1", "coefficient": 0.4, "display_name": "思考期"},
      "L3": {"primary": "S1", "secondary": "S2", "coefficient": 0.6, "display_name": "准备期"},
      "L4": {"primary": "S2", "secondary": "S3", "coefficient": 0.85, "display_name": "成长期"},
      "L5": {"primary": "S4", "secondary": "S5", "coefficient": 1.0, "display_name": "收获期"}
    },
    "behavioral_to_psychological": {
      "S0": {"levels": ["L1", "L2"], "default": "L1"},
      "S1": {"levels": ["L2", "L3"], "default": "L2"},
      "S2": {"levels": ["L3", "L4"], "default": "L3"},
      "S3": {"levels": ["L4"], "default": "L4"},
      "S4": {"levels": ["L4", "L5"], "default": "L4"},
      "S5": {"levels": ["L5"], "default": "L5"}
    }
  },

  "implicit_data_sources": {
    "description": "平台可用的隐性数据源定义，与现有数据库表对接",
    "sources": {
      "conversation": {
        "id": "CONV",
        "description": "对话内容分析",
        "table": "chat_messages",
        "fields": ["text", "sentiment", "intent", "keywords", "emotion_score", "created_at"],
        "update_frequency": "realtime",
        "extraction_window_days": 14
      },
      "task": {
        "id": "TASK",
        "description": "任务执行表现",
        "table": "user_tasks",
        "fields": ["completion_rate", "streak_days", "skip_count", "avg_completion_time", "skip_reasons"],
        "update_frequency": "daily",
        "extraction_window_days": 30
      },
      "device": {
        "id": "DEVICE",
        "description": "可穿戴设备数据",
        "table": "health_data",
        "fields": ["cgm_value", "hrv_sdnn", "hrv_rmssd", "steps", "sleep_hours", "sleep_quality"],
        "update_frequency": "realtime",
        "extraction_window_days": 7
      },
      "trigger": {
        "id": "TRIGGER",
        "description": "触发事件历史",
        "table": "trigger_records",
        "fields": ["trigger_id", "trigger_count", "risk_level", "last_trigger_time"],
        "update_frequency": "realtime",
        "extraction_window_days": 30
      },
      "interaction": {
        "id": "INTERACT",
        "description": "交互行为模式",
        "table": "user_activity",
        "fields": ["daily_active_time", "message_count", "response_latency", "app_usage_duration"],
        "update_frequency": "daily",
        "extraction_window_days": 14
      },
      "profile": {
        "id": "PROFILE",
        "description": "用户画像提取",
        "table": "user_profile",
        "fields": ["stated_goals", "value_keywords", "identity_expressions", "support_network", "preferences"],
        "update_frequency": "on_detection",
        "extraction_window_days": 90
      }
    }
  },

  "questionnaire_mapping": {
    "part1": {
      "name": "改变触发原因评估",
      "total_questions": 25,
      "total_score": 125,
      "dimensions": {
        "D1_internal_drive": {
          "name": "内在驱动力",
          "description": "来自个人内部的愿望、价值与意义",
          "max_score": 20,
          "questions": [
            {
              "q_id": "Q01",
              "text": "我意识到健康、家庭、自由等价值观变得更加重要",
              "max_score": 5,
              "implicit": {
                "source": "PROFILE",
                "method": "value_keyword_frequency",
                "config": {
                  "keywords": ["健康", "家人", "家庭", "自由", "生活质量", "有意义", "重要", "珍惜"],
                  "field": "value_keywords",
                  "window_days": 30
                },
                "scoring": {
                  "0": 1, "1-2": 2, "3-5": 3, "6-10": 4, ">10": 5
                },
                "confidence": 0.65,
                "fallback": "explicit_recommended"
              }
            },
            {
              "q_id": "Q02",
              "text": "我希望成为一个'更健康的人'或'能量型父母'",
              "max_score": 5,
              "implicit": {
                "source": "CONV",
                "method": "identity_pattern_match",
                "config": {
                  "patterns": ["我想成为", "我希望是", "我要做一个", "更好的自己", "理想中的我"],
                  "window_days": 30
                },
                "scoring": {
                  "0_matches": 1, "1_match": 2, "2_matches": 3, "3_matches": 4, ">3_matches": 5
                },
                "confidence": 0.55,
                "fallback": "explicit_required"
              }
            },
            {
              "q_id": "Q03",
              "text": "我希望自己的生活更有目的或活得更久",
              "max_score": 5,
              "implicit": {
                "source": "CONV",
                "method": "purpose_statement_detection",
                "config": {
                  "patterns": ["活久一点", "长寿", "陪伴孩子", "看到孙子", "人生目标", "为了家人"],
                  "window_days": 60
                },
                "scoring": {
                  "none": 1, "weak": 2, "moderate": 3, "strong": 4, "very_strong": 5
                },
                "confidence": 0.50,
                "fallback": "explicit_required"
              }
            },
            {
              "q_id": "Q04",
              "text": "我想对自己的身体和生活负责任",
              "max_score": 5,
              "implicit": {
                "source": "TASK",
                "method": "proactive_behavior_score",
                "config": {
                  "indicators": ["主动打卡", "主动提问", "主动记录", "提前完成"],
                  "field": "completion_rate",
                  "bonus_for_proactive": true
                },
                "scoring": {
                  "rate<0.2": 1, "0.2-0.4": 2, "0.4-0.6": 3, "0.6-0.8": 4, ">0.8": 5
                },
                "confidence": 0.75,
                "fallback": "can_infer"
              }
            }
          ]
        },
