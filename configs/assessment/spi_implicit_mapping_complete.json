    "part2": {
      "name": "心理状态层次评估",
      "total_questions": 20,
      "description": "评估面对改变时的心理准备状态",
      "levels": {
        "L1": {"name": "完全对抗", "name_en": "Total Resistance", "coefficient": 0.3},
        "L2": {"name": "抗拒与反思", "name_en": "Resistance with Reflection", "coefficient": 0.4},
        "L3": {"name": "妥协与接受", "name_en": "Selective Acceptance", "coefficient": 0.6},
        "L4": {"name": "顺应与调整", "name_en": "Adaptive Alignment", "coefficient": 0.85},
        "L5": {"name": "全面内化", "name_en": "Full Internalization", "coefficient": 1.0}
      },
      "implicit_level_determination": {
        "description": "基于综合行为数据判断心理层次",
        "primary_indicators": [
          {
            "level": "L1",
            "source": "CONV",
            "patterns": ["不想", "不行", "做不到", "别管我", "没用的", "不可能"],
            "task_indicator": {"completion_rate": "<0.1", "skip_count": ">10"},
            "weight": 1.0
          },
          {
